{
	"exclude": [".*.test.ts$"],
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": false,
			"dynamicImport": false
		},
		"transform": null,
		"target": "es5",
		"loose": false,
		"externalHelpers": true,
		"keepClassNames": false
	},
	"module": {
		"type": "commonjs",
		"strict": false,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	},
	"minify": true,
	"sourceMaps": false
}
